[run]
source = rlm
omit =
    */tests/*
    */__pycache__/*
    */site-packages/*
    */_modidx.py
    */rlmpaper/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

precision = 2
show_missing = True

[html]
directory = htmlcov
